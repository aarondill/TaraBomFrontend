{
	"name": "TaraBomFrontend",
	"version": "0.0.0",
	"private": true,
	"description": "The frontend for TaraBom",
	"repository": "https://github.com/aarondill/TaraBomFrontend",
	"license": "MIT",
	"author": "aarondill <aaronsacks2006@gmail.com>",
	"type": "module",
	"main": "dist/server.js",
	"source": "src/server.tsx",
	"scripts": {
		"build": "next build",
		"clean": "rimraf dist",
		"dev": "next dev --turbopack",
		"lint": "pnpm run '/^lint:.*/' && next lint",
		"lint:org-imports": "organize-imports-cli ./tsconfig.json",
		"lint:prettier": "prettier . --write --log-level warn",
		"lint:sort-package": "sort-package-json",
		"start": "next start"
	},
	"dependencies": {
		"next": "^15.4.5",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-pdf": "^10.0.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.32.0",
		"@next/eslint-plugin-next": "^15.4.5",
		"@types/express": "^5.0.3",
		"@types/node": "^24.1.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@typescript-eslint/parser": "^8.38.0",
		"eslint": "^9.32.0",
		"eslint-config-next": "^15.4.5",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.3.0",
		"organize-imports-cli": "^0.10.0",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"sort-package-json": "^3.4.0",
		"tsafe": "^1.8.5",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0"
	},
	"packageManager": "pnpm@10.13.1",
	"targets": {
		"main": {
			"optimize": true,
			"isLibrary": true,
			"context": "node"
		}
	}
}
