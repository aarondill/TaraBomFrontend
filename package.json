{
	"name": "TaraBomFrontend",
	"version": "0.0.0",
	"private": true,
	"description": "The frontend for TaraBom",
	"repository": "https://github.com/aarondill/TaraBomFrontend",
	"license": "MIT",
	"author": "aarondill <aaronsacks2006@gmail.com>",
	"type": "module",
	"main": "dist/server.js",
	"source": "src/server.tsx",
	"scripts": {
		"build": "parcel build",
		"clean": "rimraf dist",
		"lint": "pnpm run '/^lint:.*/'",
		"lint:eslint": "eslint . --ext .ts,.tsx --fix",
		"lint:org-imports": "organize-imports-cli ./tsconfig.json",
		"lint:prettier": "prettier . --write --log-level warn",
		"lint:sort-package": "sort-package-json",
		"start": "pnpm run build && node dist/server.js",
		"watch": "node watch.mjs 'pnpm run --silent start' src/"
	},
	"dependencies": {
		"express": "^5.1.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@parcel/transformer-inline-string": "2.15.4",
		"@types/express": "^5.0.3",
		"@types/node": "^24.1.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.3.0",
		"organize-imports-cli": "^0.10.0",
		"parcel": "^2.15.4",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"sort-package-json": "^3.4.0",
		"tsafe": "^1.8.5",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0"
	},
	"packageManager": "pnpm@10.13.1",
}
